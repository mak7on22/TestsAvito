@model IEnumerable<test.Models.Categors.Subcategory>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.SubcategoryName)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Categorys.CategoryName)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                  <a asp-action="Create" asp-controller="Modules" asp-route-id="@item.Id">  @Html.DisplayFor(modelItem => item.SubcategoryName)</a>
            </td>
            <td>
                    @Html.DisplayFor(modelItem => item.Categorys.CategoryName)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.Id">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.Id">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.Id">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
